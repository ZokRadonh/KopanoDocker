<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE template SYSTEM "template.dtd">
<template>
	<title>
		Zarafa Company
	</title>
	<regexp>
		dc=.*
	</regexp>
	<icon>
		images/ou.png
	</icon>
	<description>
		Zarafa Company
	</description>
	<askcontainer>
		1
	</askcontainer>
	<rdn>
		ou
	</rdn>
	<visible>
		1
	</visible>
	<objectClasses>
		<objectClass id="organizationalUnit">
		</objectClass>
		<objectClass id="zarafa-company">
		</objectClass>
	</objectClasses>
	<attributes>
		<attribute id="ou">
			<description>
				Unit
			</description>
			<display>
				Organisational Unit
			</display>
			<hint>
				don't include "ou="
			</hint>
			<order>
				1
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaViewPrivilege">
			<description>
				ZarafaViewPrivilege
			</description>
			<display>
				View Privilege
			</display>
			<hint>
				Companies with view privileges over selected company
			</hint>
			<order>
				4
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaSystemAdmin">
			<description>
				ZarafaSystemAdmin
			</description>
			<display>
				Admin Privilege
			</display>
			<hint>
				The user who is the system administrator for this company
			</hint>
			<order>
				3
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaQuotaOverride">
			<description>
				ZarafaQuotaOverride
			</description>
			<display>
				Quota Override
			</display>
			<hint>
				Override child quota
			</hint>
			<order>
				5
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaUserDefaultQuotaWarn">
			<description>
				zarafaUserDefaultQuotaWarn
			</description>
			<display>
				User Default Quota Warn
			</display>
			<hint>
				User default warning quota size in MB
			</hint>
			<order>
				6
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaUserDefaultQuotaSoft">
			<description>
				zarafaUserDefaultQuotaSoft
			</description>
			<display>
				User Default Quota Soft
			</display>
			<hint>
				User default soft quota size in MB
			</hint>
			<order>
				7
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaUserDefaultQuotaHard">
			<description>
				zarafaUserDefaultQuotaSoft
			</description>
			<display>
				User Default Quota Soft
			</display>
			<hint>
				User default hard quota size in MB
			</hint>
			<order>
				8
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaQuotaUserWarningRecipients">
			<description>
				zarafaQuotaUserWarningRecipients
			</description>
			<display>
				Quota User Warning Recipients
			</display>
			<hint>
				Users who will recieve a notification email when a user exceeds his quota
			</hint>
			<order>
				9
			</order>
			<page>
				1
			</page>
		</attribute>
		<attribute id="zarafaQuotaCompanyWarningRecipients">
			<description>
				zarafaQuotaCompanyWarningRecipients
			</description>
			<display>
				Quota Company Warning Recipients
			</display>
			<hint>
				Users who will recieve a notification email when a company exceeds its quota
			</hint>
			<order>
				10
			</order>
			<page>
				1
			</page>
		</attribute>
	</attributes>
</template>
